<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lisis.charles.fastreport">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lisis.charles.fastreport.DatosPersonales"/>
        <activity android:name="com.lisis.charles.fastreport.FastEmail"/>
        <activity android:name="com.lisis.charles.fastreport.HistorialAccidentes"/>
        <activity android:name="com.lisis.charles.fastreport.MisVehiculos"/>
        <activity android:name="com.lisis.charles.fastreport.Registro"/>
        <activity android:name="com.lisis.charles.fastreport.VentanaPrincipal"/>
    </application>

</manifest>